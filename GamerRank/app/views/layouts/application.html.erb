<!DOCTYPE html>
<html>
  <head>
    <title>Gamerrank</title>
    <%= csrf_meta_tags %>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-danger"><%= alert %></p>
    <% end %>
   <nav class="navbar-static-top navbar-inverse" id="toolbar" role="navigation">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-01">
      <span class="sr-only"></span>
      </button>
      <a class="navbar-brand" href="#">GamerRep</a>
    </div>
    <div class="collapse navbar-collapse" id="navbar-collapse-01">
      <ul class="nav navbar-nav">
        <li><a href="../static/index">Find a Player</a></li>
        <li><a href="../static/about">About</a></li>
      </ul>
    </div>
  </nav>
  <%= yield %>
  </body>
</html>
